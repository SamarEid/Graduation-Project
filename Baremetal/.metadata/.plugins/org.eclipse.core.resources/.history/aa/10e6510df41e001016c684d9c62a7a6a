#include "stm32f4xx_hal.h"
#include "stm32f4xx_hal_spi.h"
#include "../../communication.h"
#include "./reception.h"
extern SPI_HandleTypeDef hspi2 ;

uint8_t receptionCharReceiveCommand(){
	uint8_t command;
	HAL_SPI_Receive_DMA(&hspi2, &command, 1);
	return command;
}
